{
  "buildCommand": "npm run vercel-build || npm run build",
  "framework": "vite",
  "installCommand": null,
  "outputDirectory": "dist",
  "cleanUrls": true,
  "env": {
    "SKIP_TYPESCRIPT": "true",
    "ENABLE_TS_ERRORS": "false",
    "NODE_ENV": "production"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "trailingSlash": false,
  "github": {
    "silent": true
  }
}